digraph {

	ordering=out;
	ranksep=.4;
	bgcolor="lightgrey"; node [shape=box, fixedsize=false, fontsize=12, fontname="Helvetica-bold", fontcolor="blue"
		width=.25, height=.25, color="black", fillcolor="white", style="filled, solid, bold"];
	edge [arrowsize=.5, color="black", style="bold"]

  n0 [label="BLOCK"];
  n1 [label=":="];
  n1 [label=":="];
  n2 [label="l"];
  n3 [label="LIST"];
  n3 [label="LIST"];
  n4 [label="1"];
  n5 [label="*"];
  n5 [label="*"];
  n6 [label="3"];
  n7 [label="5"];
  n8 [label="+"];
  n8 [label="+"];
  n9 [label="FUNCALL"];
  n9 [label="FUNCALL"];
  n10 [label="e"];
  n11 [label="ARGS"];
  n12 [label="15"];
  n13 [label="FUNCALL"];
  n13 [label="FUNCALL"];
  n14 [label="f"];
  n15 [label="ARGS"];

  n0 -> n1 // "BLOCK" -> ":="
  n1 -> n2 // ":=" -> "l"
  n1 -> n3 // ":=" -> "LIST"
  n3 -> n4 // "LIST" -> "1"
  n3 -> n5 // "LIST" -> "*"
  n5 -> n6 // "*" -> "3"
  n5 -> n7 // "*" -> "5"
  n3 -> n8 // "LIST" -> "+"
  n8 -> n9 // "+" -> "FUNCALL"
  n9 -> n10 // "FUNCALL" -> "e"
  n9 -> n11 // "FUNCALL" -> "ARGS"
  n8 -> n12 // "+" -> "15"
  n3 -> n13 // "LIST" -> "FUNCALL"
  n13 -> n14 // "FUNCALL" -> "f"
  n13 -> n15 // "FUNCALL" -> "ARGS"

}

