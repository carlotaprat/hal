\frametitle{\texttt {HALTeX}}
\begin{figure}[h!]
\begin{lstlisting}[language=hal]
import haltex

section 'Use example'
enumerate:
  item; p '*First item*'
  item; p '|second|'
  item; p '**third**'
\end{lstlisting}
\begin{lstlisting}[language=output]
\section{Use example}
\begin{enumerate}
\item
\emph{First item}
\item
\texttt{second}
\item
\textbf{third}
\end{enumerate}
\end{lstlisting}
\label{haltex}
\end{figure}
